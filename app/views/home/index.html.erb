<div class="container">

  <!-- Input Panel -->
  <div class="one-quarter">

    <div class="new_entry">
      <h2>New Workout</h2>
        <input class="input_field" type="text" placeholder="Plyometrics">
        <input class="new_entry button" type="submit" value="COMMIT">
    </div>

    <div class="new_entry">
      <h2>New Exercise</h2>
        <input class="input_field" type="text" placeholder="Leglifts">
        <input class="new_entry button" type="submit" value="COMMIT">
    </div>

    <div id="do_it_for_timmy">
      <%= image_tag "otter.jpg" %>
      <p class="otter-box">Do it for Timmy!</p>
    </div>

  </div>

  <!-- Exercises Panel -->
  <div class="twenty-one-fiftieths">
    <div class="dashboard-panel">
      <div class="panel-header">
        <h1>Exercises</h1>
      </div>
        <div id="exercise_content" class="panel-content">
          <!-- Boxes get appended here -->
        </div>
        <script id="exercise_template" type="text/x-handlebars-template">
            {{#exercises}}
              <div class="exercise_box">
                <h3 class="exercise_name">
                  {{title}}
                </h3>
              </div>
            {{/exercises}}
        </script>
    </div>
  </div>
  

  <!-- Workouts Panel -->
  <div class="one-third">
    <div class="dashboard-panel">
      <div class="panel-header">
        <h1>Workouts</h1>
      </div>
      <div id="workout_content" class="panel-content">
        <!-- Boxes get appended here -->
        <script id="workout_template" type="text/x-handlebars-template">
          {{#workouts}}
            <div class="workout_box">
              <h3 class="workout_name">
                {{title}}
              </h3>
            </div>
          {{/workouts}}
        </script>
      </div>
    </div>  
  </div>

</div>

<div class="container_mobile">
  <div class="select_workout">
    <div class="select_workout_header"><h3>What's the plan?</h3></div>
    <div class="select_workout_rectangle rect_blue_dark"><h3>Back and Bi's</h3><h3 class='carrot'>></h3></div>
    <div class="select_workout_rectangle rect_blue_light"></div>
    <div class="select_workout_rectangle rect_blue_dark"></div>
    <div class="select_workout_rectangle rect_blue_light"></div>
    <div class="select_workout_rectangle rect_blue_dark"></div>
    <div class="select_workout_rectangle rect_blue_light"></div>
    <div class="select_workout_menu">
      <div class="select_workout_menu_button"><h3>Menu</h3></div>
    </div>
  </div>

  

</div>